<h1>{{categorie.name}}</h1>
{{#each posts}}
<div class="card mb-2">
    <div class="card-body">
        <h3>{{title}}</h3>
        <p>{{description}}</p>
        <a href="/post/{{slug}}"><button class="btn btn-success">See more</button></a>
        <hr>
        <br>
        <small>Date: {{date}}</small>
    </div>
</div>
{{else}}
    <h5>No one posts!</h5>
{{/each}}